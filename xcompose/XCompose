include "%L"

# German Stuff
<Multi_key> <a> <a>	: "ä"	# Umlaut a
<Multi_key> <o> <o>	: "ö"	# Umlaut o
<Multi_key> <u> <u>	: "ü"	# Umlaut u
<Multi_key> <A> <A>	: "Ä"	# Umlaut A
<Multi_key> <O> <O>	: "Ö"	# Umlaut O
<Multi_key> <U> <U>	: "Ü"	# Umlaut U
<Multi_key> <s> <s>	: "ß"	U03B1 # Esszet

# Greek Letters
